package hello;

public class UtilNote {
	
	int[] notes;

	public double calcMoyenne(int[] notes){
		int somme = 0;

		for (int i = 0; i < notes.length; i++) {
			somme += notes[i];
		}
		double moyenne = 1.0 * somme / notes.length; //oubien on fait (double) somme / notes.length;
		return moyenne;

	}

}
