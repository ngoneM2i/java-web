package hello;

public class Personne {
	//variables d'instance
	//intance fileds ou champs
	String prenom;
	String nom;
	int age;
	
	public String getFullName() {
		return prenom + " " + nom;
	}

	public String getPrenom() {
		
		return prenom;
	}

	public void setPrenom(String newPrenom) {
		prenom = newPrenom;
		
	}

}
